{
   "include": ["index.ts"],
   "compilerOptions": {
      "target": "ES2016",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "isolatedModules": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "alwaysStrict": true,
      "noImplicitAny": true,
		"strictFunctionTypes": false,
      "skipLibCheck": false,
      "noEmitOnError": true,
      "noEmit": true
   }
}
