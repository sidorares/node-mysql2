    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.83.0"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.7.0",
